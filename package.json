{
  "name": "labor-cli",
  "description": "CLI interface for managing your tasks on Labor",
  "version": "0.1.0",
  "author": "Yurick @Yurickh",
  "bin": {
    "labor": "./bin/run"
  },
  "bugs": "https://github.com/Yurickh/labor-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/inquirer": "^0.0.43",
    "@types/listr": "^0.13.0",
    "@types/node-fetch": "^2.1.4",
    "inquirer": "^6.2.1",
    "isemail": "^3.2.0",
    "keytar": "^4.3.0",
    "listr": "^0.14.3",
    "node-fetch": "^2.3.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/jest": "^24.0.6",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "@typescript-eslint/parser": "^1.4.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "nock": "^10.0.5",
    "oclif": "^1.12.8",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-node": "^7",
    "typescript": "^3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Yurickh/labor-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "labor",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Yurickh/labor-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint --ext .ts .",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint --ext .ts ."
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ],
    "*.{ts, js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    }
  }
}
